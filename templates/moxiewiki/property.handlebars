<div class="api propertydetails">
	<div class="syntax"><span>{{syntax}}</span></div>
	<div class="details">{{desc}}</div>

	{{#if examples}}
		<div class="examples">
		<h2>Examples</h2>
		{{#each examples}}
			<pre class="js">{{content}}</pre>
		{{/each}}
		</div>
	{{/if}}

	<div class="property">
		<h2>Type</h2>
		<div>
			{{#each types}}
				{{#if link}}
					<a href="{{link}}">{{name}}</a>
				{{else}}
					{{name}}
				{{/if}}
			{{/each}}

			{{desc}}
		</div>
	</div>
</div>